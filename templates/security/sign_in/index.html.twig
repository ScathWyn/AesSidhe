{% extends 'base.html.twig' %}

{% block main %}
<div class="row">
	<div class="col-md-4">
		{% if error %}
			<div class="alert alert-danger">
				{{ error.messageKey|trans(error.messageData, 'security') }}
			</div>
		{% endif %}
		{{ form_start(form) }}
		{{ form_widget(form) }}
		{{ form_end(form) }}
	</div>
</div>
{% endblock %}